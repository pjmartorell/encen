<%= title t(".title") %>
<%= meta_keywords t(".metakeywords") %>
<%= meta_description t(".metadescription") %>

<div id="post_header">
  <h1><%= raw t('.header') %></h1>
</div>

<div id="services">
  <h2><%= raw t('.subtitle') %></h2>

  <div id="services_descritpion">
    <div class="separator"></div>

    <div class="image_description"><img alt="Limpieza" src="images/clean.png"/></div>
    <div class="description">
      <h3 id="clean"><a href="<%= limpieza_path %>"><%= raw t('.line1') %></a></h3>
      <p><%= raw t('.line2') %></p>
    </div>

    <div class="line_separation"></div>

    <div class="image_description"><img alt="Conserjeria" src="images/manager.png"/></div>
    <div class="description">
      <h3 id="manager"> <a href="<%= conserjeria_path %>"><%= raw t('.line3') %></a> </h3>
      <p><%= raw t('.line4') %></p>
    </div>

    <div class="line_separation"></div>

    <div class="image_description"><img alt="garden" src="images/garden.png"/></div>
    <div class="description">
      <h3 id="garden"><a href="<%= jardineria_path %>"><%= raw t('.line5') %></a> </h3>
      <p><%= raw t('.line6') %></p>
    </div>

    <div class="line_separation"></div>

    <div class="image_description"><img alt="pool" src="images/pool.png"/></div>
    <div class="description">
      <h3 id="pool"><a href="<%= piscinas_path %>"><%= raw t('.line7') %></a></h3>
      <p><%= raw t('.line8') %></p>
    </div>

    <div class="separator"></div>
  </div>

</div>
